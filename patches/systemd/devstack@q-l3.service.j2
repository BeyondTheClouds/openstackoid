
[Unit]
Description = Devstack devstack@q-l3.service

[Service]
Environment="HTTP_PROXY=http://{{ the_conf.ip}}:8888"
ExecReload = /bin/kill -HUP $MAINPID
TimeoutStopSec = 300
KillMode = process
ExecStart = /usr/local/bin/neutron-l3-agent --config-file /etc/neutron/neutron.conf --config-file /etc/neutron/l3_agent.ini
User = stack

[Install]
WantedBy = multi-user.target
